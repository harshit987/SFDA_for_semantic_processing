rn entropy\n","\n","def calculate_weights(input,c=1):\n","  prob = nn.Softmax(dim=1)(input).detach()\n","  ent = Entropy(prob)\n","  # weights = c*(1+(1/np.log(2)))*((1/(1+ent))-(1/(1+np.log(2))))\n","  weights = ent.detach().clone()\n","  weights[ent<=c] = 1.0\n","  weights[ent>c] = 0.0\n","  return weights"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"jujXSwMN8XEe"},"source":["def mean_crossentropy(y_logits,y_true):\n","  return nn.CrossEntropyLoss()(y_logits[0],y_true[0])"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"hxsa7JeGxCxC"},"source":["def train_target(loader, target_net, epochs=5, lr=0.01, save=False, save_path='/content/drive/My Drive/SFDA/Time/model/'):\n","    # target_net = target_net.cuda()\n","    target_net.zero_grad()\n","\n","    for i,[name,param] in enumerate(target_net.named_parameters()):\n","        if i>198: #or i<165: ### i>196: pooler+classifier; i<133: first 8 layers \n","            param.requires_grad=True\n","        else:\n","            param.requires_grad=True\n","\n","    param_group = []\n","    for name, param in target_net.named_parameters():\n","        if param.requires_grad:\n","            param_group += [{'params': param, 'lr': lr}]\n","\n","    optimizer = optim.SGD(param_group)\n","    optimizer = op_copy(optimizer)\n","\n","    epoch_loss=0.0\n","    max_iter = epochs * len(loader)\n","    interval_iter = len(loader)\n","    iter_num = 0\n","\n","    # scores=[]\n","    # scores.append([0,0.834019,0.850746,0.817937,912,203,4270,160])\n","\n","    start=True\n","\n","    while iter_num < max_iter:\n","        optimizer.zero_grad()\n","        \n","        print(iter_num // interval_iter,iter_num % interval_iter,'completed')\n","\n","        iter_num += 1\n","        lr_scheduler(optimizer, iter_num=iter_num, max_iter=max_iter)\n","\n","        try:\n","            input_id, attention_mask = iter_test.next()\n","        except:\n","            iter_test = iter(